<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">

	<navigation-rule>
		<display-name>lector_list.xhtml</display-name>
		<from-view-id>/lector_list.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>new</from-outcome>
			<to-view-id>/lector_new.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>lector_new.xhtml</display-name>
		<from-view-id>/lector_new.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>insert</from-outcome>
			<to-view-id>/lector_list.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>lector_list.xhtml</display-name>
		<from-view-id>/lector_list.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>delete</from-outcome>
			<to-view-id>/lector_list.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>lector_list.xhtml</display-name>
		<from-view-id>/lector_list.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/lector_edit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>lector_edit.xhtml</display-name>
		<from-view-id>/lector_edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>update</from-outcome>
			<to-view-id>/lector_list.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>lector_screen.xhtml</display-name>
		<from-view-id>/lector_screen.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/lector_profile_edit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>lector_screen.xhtml</display-name>
		<from-view-id>/lector_screen.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>courses_list</from-outcome>
			<to-view-id>/lector_opened_courses.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>lector_edit.xhtml</display-name>
		<from-view-id>/lector_edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/lector_list.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>lector_profile_edit.xhtml</display-name>
		<from-view-id>/lector_profile_edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/lector_screen.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>lector_profile_edit.xhtml</display-name>
		<from-view-id>/lector_profile_edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>update</from-outcome>
			<to-view-id>/lector_screen.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login_lector</from-outcome>
			<to-view-id>/lector_screen.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>

</faces-config>
